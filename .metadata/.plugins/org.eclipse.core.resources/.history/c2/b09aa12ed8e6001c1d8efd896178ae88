<%@page import="java.net.URLDecoder"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>새 탭</title>
</head>
<body>
	<%
		Cookie[] cok = request.getCookies();
		if (cok != null && cok.length > 0) {
			for (int i = 0; i < cok.length; i++) {
				if (cok[i].getName().equals("DCTCookie")) {
					switch (URLDecoder.decode(cok[i].getValue(), "utf-8")) {
					case ("cookieoffscreen"): {
						response.sendRedirect("secretTabOff.jsp");
					}
					case ("cookieonscreen"): {
						response.sendRedirect("secretTabOn.jsp");
					}
					default: {
						System.out.println("hi");
						response.sendRedirect("secretTabFirst.jsp");
					}
					}
				}
				else{
					System.out.println("hi1");
					%>
					<jsp:forward page="secretTabFirst.jsp" />
					<%
					response.sendRedirect("secretTabFirst.jsp");
				}

			}
		} else {
			System.out.println("hi2");
			response.sendRedirect("secretTabFirst.jsp");
		}
	%>
</body>
</html>